{
    "name": "mosaic-react",
    "version": "1.0.0",
    "description": "Hooks for using Mosaic with React",
    "keywords": [
        "mosaic",
        "data-viz"
    ],
    "repository": {
        "url": "github:trackagree/mosaic-react"
    },
    "license": "MPL-2.0",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "browser": "./dist/index.js",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "tsup",
        "copy-next": "cp -r dist/public/*.js ../../public",
        "dev": "tsup --watch"
    },
    "devDependencies": {
        "@swc/core": "^1.3.66",
        "@types/node": "^20.3.1",
        "tsup": "7.1.0"
    }
}
